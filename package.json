{
	"name": "bettingbot",
	"version": "2.5.1",
	"description": "Pluto Betting App",
	"main": "./dist/Pluto.js",
	"module": "./dist/Pluto.js",
	"types": "./dist/Pluto.d.cts",
	"exports": {
		"require": {
			"types": "./dist/Pluto.d.cts",
			"default": "./dist/Pluto.cjs"
		},
		"import": {
			"types": "./dist/Pluto.d.mts",
			"default": "./dist/Pluto.mjs"
		}
	},
	"bin": "./dist/cli.js",
	"prettier": "./.prettierrc.json",
	"scripts": {
		"build": "tsc --project ./tsconfig.json",
		"watch": "nodemon -L --watch ./ --exec npm run dev",
		"start": "node ./dist/Pluto.js",
		"format": "prettier --write \"src/**/*.js\"",
		"test": "ava",
		"docs": "jsdoc2md lib/*.js > addNewBet.js",
		"auto": "nodemon node ./dist/Pluto.js",
		"nflc": "cross-env NODE_ENV=production SEASON_TYPE=REGULAR SERVERNAME=nfl SPORT=nfl EXP_PORT=4442 node --loader esm-module-alias/loader --no-warnings ./dist/Pluto.js -r dotenv/config dotenv_config_path=./.env.nflc",
		"nbac": "cross-env NODE_ENV=production SEASONTYPE=REGULAR SERVERNAME=nba SPORT=nba EXP_PORT=4443  node --loader esm-module-alias/loader --no-warnings ./dist/Pluto.js -r dotenv/config dotenv/config dotenv_config_path=./.env.nbac ",
		"dev": "cross-env SERVERNAME='nba-dev' SEASON_TYPE=REGULAR node --loader esm-module-alias/loader --no-warnings ./dist/Pluto.js -r dotenv/config ./Pluto.js dotenv_config_path=./.env.dev",
		"vault-open": "npx dotenv-vault@latest open",
		"vault-login": "npx dotenv-vault@latest login",
		"vault-push": "npx dotenv-vault@latest push",
		"vault-pull": "npx dotenv-vault@latest pull"
	},
	"type": "module",
	"author": "fearandesire",
	"license": "ISC",
	"aliases": {
		"@pluto-api-routes": "./dist/utils/api/routes",
		"@pluto-betOps": "./dist/utils/db/betOps",
		"@pluto-betOps-modifyAmount": "./dist/utils/db/betOps/modifyAmount.js",
		"@pluto-bettingStats": "./dist/utils/bot_res/getBettingStats.js",
		"@pluto-check-balance": "./dist/utils/db/validation/checkbalance.js",
		"@pluto-core": "./dist/Pluto.js",
		"@pluto-core-config": "./dist/lib/PlutoConfig.js",
		"@pluto-currency": "./dist/utils/db/currency",
		"@pluto-date-utils": "./dist/utils/date",
		"@pluto-db": "./dist/database/dbindex.js",
		"@pluto-db-utils": "./dist/utils/db",
