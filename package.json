	"jest": {
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1",
			"^lib/(.*)$": "<rootDir>/lib/$1"
		},
		"moduleFileExtensions": ["ts", "js"],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": ["**/*.(t|j)s"],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
