{
	"name": "pluto-betting",
	"version": "3.5.0",
	"description": "Sports Betting Application for Discord",
	"type": "module",
	"license": "MIT",
	"private": true,
	"author": "fenix <fenixcoding@gmail.com>",
	"email": "fenixcoding@gmail.com",
