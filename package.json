{
	"name": "bettingbot",
	"version": "2.5.0",
	"description": "Pluto Betting App",
	"main": "./Pluto.mjs",
	"exports": "./Pluto.mjs",
	"prettier": "./.prettierrc.json",
	"scripts": {
		"watch": "nodemon -L --watch ./ --exec npm run dev",
		"start": "node ./Pluto.js",
		"format": "prettier --write \"src/**/*.js\"",
		"test": "ava",
		"docs": "jsdoc2md lib/*.js > addNewBet.js",
		"auto": "nodemon node ./Pluto.mjs",
		"nflc": "cross-env NODE_ENV=production SEASON_TYPE=REGULAR SERVERNAME=nfl SPORT=nfl EXP_PORT=4442 node --loader esm-module-alias/loader --no-warnings ./Pluto.mjs -r dotenv/config dotenv_config_path=./.env.nflc",
		"nbac": "cross-env NODE_ENV=production SEASONTYPE=REGULAR SERVERNAME=nba SPORT=nba EXP_PORT=4443  node --loader esm-module-alias/loader --no-warnings ./Pluto.mjs -r dotenv/config dotenv/config dotenv_config_path=./.env.nbac ",
		"dev": "cross-env SERVERNAME='nba-dev' SEASON_TYPE=REGULAR node --loader esm-module-alias/loader --no-warnings ./Pluto.mjs -r dotenv/config ./Pluto.mjs dotenv_config_path=./.env.dev",
		"vault-open": "npx dotenv-vault@latest open",
		"vault-login": "npx dotenv-vault@latest login",
		"vault-push": "npx dotenv-vault@latest push",
		"vault-pull": "npx dotenv-vault@latest pull"
	},
	"type": "module",
	"author": "fearandesire",
	"license": "ISC",
	"aliases": {
		"@pluto-server-config": "./utils/serverConfig.js",
		"@pluto-core-config": "./lib/PlutoConfig.js",
		"@pluto-check-balance": "./utils/db/validation/checkbalance.js",
