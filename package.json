{
	"name": "bettingbot",
	"version": "2.7.4",
	"description": "Pluto Betting App",
	"main": "./dist/Pluto.js",
	"module": "./dist/Pluto.js",
	"types": "./dist/Pluto.d.cts",
	"exports": {
		"require": {
			"types": "./dist/Pluto.d.cts",
			"default": "./dist/Pluto.cjs"
		},
		"import": {
			"types": "./dist/Pluto.d.mts",
			"default": "./dist/Pluto.mjs"
		}
	},
	"bin": "./dist/cli.js",
	"prettier": "./.prettierrc.json",
	"scripts": {
		"build": "rimraf ./dist && tsc",
		"watch": "nodemon -L --watch ./ --exec npm run dev",
		"start": "node ./dist/Pluto.js",
		"dev": "cross-env NODE_ENV=development node --loader esm-module-alias/loader --no-warnings ./dist/Pluto.js",
		"ts-dev": "tsx ./src/Pluto.ts",
		"gen-api": "openapi-generator-cli generate -i ./Khronos-Swagger-Spec-v1.json -g typescript-fetch -o ./src/openapi/khronos -c openapi.config.json",
		"post-api-fix": "node ./src/utils/scripts/addJsExtension.js",
		"ci-gen-api": "rimraf ./src/openapi && yarn gen-api && yarn post-api-fix",
		"sap-cmd": "sapphire generate slashcommand",
		"dvault": "npx dotenv-vault@latest"
	},
	"type": "module",
	"author": "fearandesire",
	"license": "ISC",
	"aliases": {
		"@khronos-index": "./dist/openapi/khronos/index.js",
		"@pluto-api-routes": "./dist/utils/api/routes",
		"@pluto-betOps": "./dist/utils/db/betOps",
		"@pluto-betOps-modifyAmount": "./dist/utils/db/betOps/modifyAmount.js",
		"@pluto-bettingStats": "./dist/utils/bot_res/getBettingStats.js",
		"@pluto-check-balance": "./dist/utils/db/validation/checkbalance.js",
		"@pluto-core": "./dist/Pluto.js",
		"@pluto-core-config": "./dist/lib/PlutoConfig.js",
		"@pluto-currency": "./dist/utils/db/currency",
		"@pluto-date-utils": "./dist/utils/date",
		"@pluto-db": "./dist/database/dbindex.js",
		"@pluto-db-utils": "./dist/utils/db",
		"@pluto-embed-reply": "./dist/utils/bot_res/embeds/embedReply.js",
		"@pluto-general-utils": "./dist/utils/bot_res",
		"@pluto-internal-color-logger": "./dist/utils/bot_res/ColorConsole.js",
		"@pluto-internal-logger": "./dist/utils/bot_res/consoleLog.js",
		"@pluto-iso-manager": "./dist/utils/time/IsoManager.js",
		"@pluto-leaderboard": "./dist/utils/betting/currency/leaderboard.js",
		"@pluto-list-user-bets": "./dist/utils/db/listMyBets.js",
		"@pluto-logger": "./dist/utils/PlutoLogger.js",
		"@pluto-matchupOps": "./dist/utils/db/matchupOps.js",
		"@pluto-newBet": "./dist/utils/db/betOps/newBet.js",
		"@pluto-process-claim": "./dist/utils/db/betOps/processClaim.js",
		"@pluto-redis": "./dist/utils/cache/RedisCacheManager.js",
		"@pluto-reply": "./dist/utils/bot_res/reply.js",
		"@pluto-server-config": "./dist/utils/serverConfig.js",
		"@pluto-stats-embed-builder": "./dist/utils/bot_res/statsEmbBuilder.js",
		"@pluto-transfer": "./dist/utils/db/currency/transferBetween.js",
		"@pluto-validate": "./dist/utils/db/validation",
		"@pluto-validate-betAuthor": "./dist/utils/db/validation/verifyBetAuthor.js",
		"@pluto-validate-checkbalance": "./dist/utils/db/validation/checkbalance.js",
		"@pluto-validate-user": "./dist/utils/db/validation/validateExistingUser.js"
	},
	"dependencies": {
		"@favware/colorette-spinner": "^1.0.1",
		"@kaname-png/plugin-statcord": "^2.1.4",
		"@koa/cors": "^4.0.0",
		"@openapitools/openapi-generator-cli": "^2.9.0",
		"@sapphire/decorators": "^6.0.3",
		"@sapphire/discord-utilities": "^3.2.1",
		"@sapphire/discord.js-utilities": "^7.1.5",
		"@sapphire/fetch": "^3.0.1",
		"@sapphire/framework": "^5.0.6",
		"@sapphire/pieces": "^3.3.3",
		"@sapphire/plugin-api": "^6.1.0",
		"@sapphire/plugin-editable-commands": "^4.0.1",
		"@sapphire/plugin-hmr": "^2.0.0",
		"@sapphire/plugin-logger": "^4.0.1",
		"@sapphire/plugin-subcommands": "^6.0.2",
		"@sapphire/ratelimits": "^2.4.7",
		"@sapphire/time-utilities": "^1.7.11",
		"@sapphire/type": "^2.4.3",
		"@sapphire/utilities": "^3.15.2",
		"@skyra/env-utilities": "^1.3.0",
		"@types/koa": "^2.14.0",
		"@types/koa-logger": "^3.1.5",
		"@types/koa__cors": "^5.0.0",
		"accounting": "^0.4.1",
		"axios": "^1.2.3",
		"body-parser": "^1.20.2",
		"color": "^4.2.3",
		"color-convert": "^2.0.1",
		"colorette": "^2.0.20",
		"cross-env": "^7.0.3",
		"date-fns": "^2.29.3",
		"date-fns-tz": "^2.0.0",
		"datejs": "^1.0.0-rc3",
		"debug": "^4.3.4",
		"discord.js": "^14.14.1",
		"dotenv": "16.4.5",
		"dotenv-vault": "1.26.1",
		"esm-module-alias": "^2.1.0",
		"express": "^4.18.2",
		"express-async-router": "^0.1.15",
		"express-promise-router": "^4.1.1",
		"express-status-monitor": "^1.3.4",
		"fuse.js": "^6.6.2",
		"ioredis": "^5.3.2",
		"koa": "^2.14.2",
		"koa-bodyparser": "^4.4.1",
		"koa-logger": "^3.2.1",
		"koa-router": "^12.0.1",
		"lodash": "^4.17.21",
		"lodash.pullat": "^4.6.0",
		"module-alias": "^2.2.3",
		"nodemon": "^2.0.19",
		"numeral": "^2.0.6",
		"pg": "^8.7.3",
		"pg-monitor": "^1.4.1",
		"pg-promise": "^10.11.1",
		"pkg-dir": "^7.0.0",
		"pm2": "^5.2.0",
		"puppeteer": "^19.8.0",
		"puppeteer-extra": "^3.3.6",
		"puppeteer-extra-plugin-recaptcha": "^3.6.8",
		"puppeteer-extra-plugin-stealth": "^2.11.2",
		"resolve-team": "^1.6.0",
		"statcord.js": "^3.4.3",
		"winston": "^3.8.2",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"devDependencies": {
		"@sapphire/cli": "^1.9.1",
		"@sapphire/prettier-config": "^2.0.0",
		"@sapphire/ts-config": "^5.0.0",
		"@types/color": "^3.0.6",
		"@types/jest": "^29.5.12",
		"@types/koa-bodyparser": "^4.3.12",
		"@types/koa-router": "^7.4.8",
		"@types/lodash": "^4.14.202",
		"@types/node": "^20.11.5",
		"@types/node-cron": "^3.0.11",
		"@types/numeral": "^2.0.5",
		"@types/ws": "^8.5.10",
		"@typescript-eslint/eslint-plugin": "^6.19.0",
		"@typescript-eslint/parser": "^6.19.0",
		"bluebird": "^3.7.2",
		"eslint": "8.22.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-node": "^0.3.7",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-mocha": "^10.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"jest": "^29.7.0",
		"npm-run-all": "^4.1.5",
		"pkgroll": "^2.0.1",
		"prettier": "^3.2.4",
		"prettier-eslint-cli": "^7.1.0",
		"rimraf": "^5.0.5",
		"ts-jest": "^29.1.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.2"
	},
	"jest": {
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1",
			"^lib/(.*)$": "<rootDir>/lib/$1"
		},
		"moduleFileExtensions": [
			"ts",
			"js"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
